<!DOCTYPE html>
<html>
<head>
  <title>Texting App</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <%= render 'layouts/navbar' %>
  <div class="container">
    <h1>Texting App</h1>
    <div id="flashes">
      <%= flash[:notice] %>
      <%= flash[:alert] %>
    </div>
    <%= yield %>
  </div>
</body>
</html>
